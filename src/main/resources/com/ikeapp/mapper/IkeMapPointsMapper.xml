<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ikeapp.mapper.IkeMapPointsDao">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.ikeapp.entity.IkeMapPoints">
		<id column="ID" property="id" />
		<result column="LNG" property="lng" />
		<result column="LAT" property="lat" />
		<result column="TYPE_ST" property="typeSt" />
		<result column="MAP_ORDER" property="mapOrder" />
		<result column="SYS_CREATE_DATE" property="sysCreateDate" />
		<result column="VERSION" property="version" />
		<result column="DISABLED" property="disabled" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID AS id, LNG AS lng, LAT AS lat, TYPE_ST AS typeSt, MAP_ORDER AS mapOrder, SYS_CREATE_DATE AS sysCreateDate, VERSION AS version, DISABLED AS disabled
    </sql>

</mapper>
