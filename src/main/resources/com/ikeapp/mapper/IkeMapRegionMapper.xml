<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ikeapp.mapper.IkeMapRegionDao">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.ikeapp.entity.IkeMapRegion">
		<id column="ID" property="id" />
		<result column="REGION_NAME" property="regionName" />
		<result column="REGION_CODE" property="regionCode" />
		<result column="LEVEL_ST" property="levelSt" />
		<result column="CREATE_DATE" property="createDate" />
		<result column="PRICE" property="price" />
		<result column="TOP_PRICE" property="topPrice" />
		<result column="POPULATION_ST" property="populationSt" />
		<result column="SYS_CREATE_DATE" property="sysCreateDate" />
		<result column="VERSION" property="version" />
		<result column="DISABLED" property="disabled" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID AS id, REGION_NAME AS regionName, REGION_CODE AS regionCode, LEVEL_ST AS levelSt, CREATE_DATE AS createDate, PRICE AS price, TOP_PRICE AS topPrice, POPULATION_ST AS populationSt, SYS_CREATE_DATE AS sysCreateDate, VERSION AS version, DISABLED AS disabled
    </sql>

</mapper>
