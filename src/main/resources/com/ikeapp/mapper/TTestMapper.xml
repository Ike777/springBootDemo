<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ikeapp.mapper.TTestDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ikeapp.entity.TTest">
        <id column="id" property="id" />
        <result column="mobile" property="mobile" />
        <result column="real_name" property="realName" />
        <result column="introduce" property="introduce" />
        <result column="status" property="status" />
        <result column="created_by" property="createdBy" />
        <result column="created_date" property="createdDate" />
        <result column="last_modified_by" property="lastModifiedBy" />
        <result column="last_modified_date" property="lastModifiedDate" />
        <result column="tttt" property="tttt" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, mobile, real_name AS realName, introduce, status, created_by AS createdBy, created_date AS createdDate, last_modified_by AS lastModifiedBy, last_modified_date AS lastModifiedDate, tttt
    </sql>

</mapper>
